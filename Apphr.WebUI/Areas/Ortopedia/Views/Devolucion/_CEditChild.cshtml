@model Apphr.Application.Ortopedia.Devolucion.DTOs.DevolucionDTOCEditChild
@Html.AntiForgeryToken()
@Html.HiddenFor(x => x.ProveedorId)
@Html.HiddenFor(x => x.DevolucionId)
@Html.HiddenFor(x => x.PacienteId)
@Html.HiddenFor(x => x.Fecha)
<div class="row">
    <div class="col-md-4">@Html.EditorFor(x => x.HojaGastoId, new { optionsList = ViewBag.HojaGastoId })</div>    
    <div class="col-md-4">@Html.EditorFor(x => x.HGFechaFormulario)</div>
    <div class="col-md-4">@Html.EditorFor(x => x.HGRegistroMedico)</div>
    <div class="col-md-12">@Html.EditorFor(x => x.HGPaciente)</div>

    <div class="col-md-4">@Html.EditorFor(x => x.MaterialId, new { optionsList = ViewBag.MaterialId })</div>
    <div class="col-md-8">@Html.EditorFor(x => x.MaterialNombre)</div>
    <div class="col-md-4">@Html.EditorFor(x => x.Cantidad)</div>
    <div class="col-md-4">@Html.EditorFor(x => x.Precio)</div>
    <div class="col-md-4">@Html.EditorFor(x => x.Valor)</div>
</div>